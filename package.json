{
  "name": "ol-wip",
  "version": "1.0.0",
  "description": "Open Layers - Web Interactive Map",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run services\" \"npm run dev-server\"",
    "build": "rimraf build && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build-watch": "rimraf build && webpack -w",
    "services": "nodemon -w ./services ./services/server.js",
    "dev-server": "webpack-dev-server --inline --hot --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denisaangelov/ol-wip.git"
  },
  "keywords": [
    "Open",
    "Layers",
    "react",
    "webpack",
    "babel"
  ],
